<pre class=3D"theme: Confluence; brush: java; gutter: false" style=3D"font-=
size:12px;"># Load balance UDP-based DNS traffic across two servers
stream {
    log_format  main  '[$time_local] - remote: [$remote_addr:$remote_port] =
'
                      '- server: [$server_addr:$server_port] '
                      '- bytes_sent: $bytes_sent '
                      '- bytes_received: $bytes_received '
                      '- front4bytes: $front4bytes '
                      '- protocol: $protocol';
    access_log  logs/stream.log  main;
    upstream udp_upstreams {
        # hash =E8=87=AA=E5=AE=9A=E4=B9=89=E7=9A=84=E5=AD=97=E6=AE=B5
        hash $front4bytes;
        server 127.0.0.1:19999;
        server 127.0.0.1:29999;
    }  =20
    server {
        listen 9999 udp;
        proxy_pass udp_upstreams;
        proxy_timeout 1s;=20
        proxy_responses 1;
        error_log logs/error.log;
    }  =20
}</pre>=20
